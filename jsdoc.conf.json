{
  "tags": {
	"allowUnknownTags": true,
	"dictionaries": ["jsdoc", "closure"]
  },
  "source": {
	"include": ["src"],
	"includePattern": ".+\\.js(doc|x)?$",
	"excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
	"plugins/markdown"
  ],
  "templates": {
	"better-docs": {
	  "name": "CLOWDER 2.0 frontend"
	}
  },
  "opts": {
	"destination": "docs",
	"recurse": true,
	"readme": "README.md",
	"template": "node_modules/better-docs"
  }
}
